.listWithScroll {
  display: flex;
  overflow: auto;
  flex-direction: column;
  flex: 1;
  align-items: center;

  &::-webkit-scrollbar {
    width: 0;
    height: 0;
  }

  & > ul {
    margin: 0 10px;
    display: flex;
    flex-direction: column;
    width: 94%;
    text-align-last: center;
  }
}
